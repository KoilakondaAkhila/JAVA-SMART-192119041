import java.util.*;
public class Main{
    public static void main(String[]args){
        Scanner sc = new Scanner(System.in);
        System.out.print("enter the number:");
        int a= sc.nextInt();
        for(int i=2;i<a;i++)
        {
            while(a%i==0)
            {
              System.out.print(i+" "); 
              a=a/i;
            }
        }
    }
}
