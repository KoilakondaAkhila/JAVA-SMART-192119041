public class Main{
    public static void main(String[]args){
        int[] n=new int[args.length];
        for(int i=0;i<args.length;i++){
            try{
                n[i]=Integer.parseInt(args[i]);
            }catch(NumberFormatException e){
                System.out.print("Invalid input");
                return;
            }
        }int sum=0;
        for(int i=0;i<args.length;i++){
            sum=sum+n[i];
        }System.out.print("Sum: "+sum);
        
    }
}
